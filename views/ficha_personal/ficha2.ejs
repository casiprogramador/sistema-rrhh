                  <ul class="nav nav-tabs flex-column flex-sm-row" id="FichaPersonalTab" role="tablist">
                    <li class="nav-item">
                      <a class="nav-link" id="datospersonalestab" data-toggle="tab" href="
                      #identificacionpersonal" role="tab" aria-controls="identificacionpersonal">Datos Laborales I</a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" id="datospersonales1tab" data-toggle="tab" href="#datospersonales1" role="tab" aria-controls="datospersonales1">Datos Laborales II</a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" id="datospersonales2tab" data-toggle="tab" href="#datospersonales2" role="tab" aria-controls="datospersonales2">Estudios</a>
                    </li>
                  </ul>
                </div>
                <div class="wrapper">
