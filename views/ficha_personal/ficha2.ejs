                  <div class="tab-content" id="myTabContent">
                    <!-- tab datos laborales 1 -->
                    <div class="tab-pane fade show active" id="identificacionpersonal" role="tabpanel" aria-labelledby="profile-tab">
                      <div class="container">
                      <h1 class="card-title">Datos laborales I </h1>
                        <!-- tab datospersonales1 ends -->


                          <div class="row">
                            <div class="col">
                            <div class="form-group">
                            <label for="name">Numero Nit</label>
                            <input name="nit" type="text" class="form-control" id="nit" placeholder="Nit" onkeypress="return valida(event)" minlength="2" value="<%=(newemp[0].nit!=null) ? newemp[0].nit:'0' %>">
                            </div>
                            </div>
                            <div class="col">
                            <div class="form-group">
                            <label for="designation">Nombre AFP</label>
                            <input name="afp" type="text" class="form-control" id="afp" placeholder="Nombre AFP" minlength="2" value="<%=(newemp[0].afp!=null) ? newemp[0].afp:'' %>">
                            </div>
                            </div>
                            </div>


                            <div class="row">
                            <div class="col">
                            <div class="form-group">
                              <label for="seguro_medico">Nombre Seguro Medico</label>
                              <input name="seguro_medico" type="text" class="form-control" id="seguro_medico" placeholder="Seguro Medico" minlength="2" value="<%=(newemp[0].seguro_medico!=null) ? newemp[0].seguro_medico:'' %>">
                            </div>
                            </div>
                            <div class="col">
                            <div class="form-group">
                            <label for="nro_seguro">Numero de Seguro</label>
                            <input name="nro_seguro" class="form-control" id="num_seguro" placeholder="Numero Seguro medico" onkeypress="return valida(event)" minlength="2" value="<%=(newemp[0].nro_seguro!=null) ? newemp[0].nro_seguro:'0' %>">
                            </div>
                            </div>
                            </div>


                                    <div class="row">
                                    <div class="col">
                                    <div class="form-group">
                                    <label for="tipo_seguro">Tipo de Seguro</label>
                                    <select name="tipo_seguro" class="form-control" style="width:100%">
                                      <option value="<%=(newemp[0].tipo_seguro!=null) ? newemp[0].tipo_seguro:'' %>"><%=(newemp[0].tipo_seguro!=null) ? newemp[0].tipo_seguro:'' %></option>>
                                    <option value="Privado">Privado</option>
                                    <option value="Publico">Publico</option>
                                    </select>
                                    </div>
                                    </div>
                                    <div class="col">
                                    <div class="form-group">
                                      <label for="name">Numero Unico de Afiliado NUA o CUA</label>
                                      <input name="nua" type="text" class="form-control" id="nua" placeholder="Numero Unico de Afiliado" onkeypress="return valida(event)" minlength="2" value="<%=(newemp[0].nua!=null) ? newemp[0].nua:'0' %>">
                                    </div>
                                    </div>
                                    </div>


                                    <div class="row">
                                            <div class="col">
                                            <div class="form-group">
                                              <label for="ren">Numero REN (exclusivo caso rentista)</label>
                                              <input name="ren" type="text" class="form-control" id="ren" placeholder="Numero REN" onkeypress="return valida(event)" minlength="2" value="<%=(newemp[0].ren!=null) ? newemp[0].ren:'0' %>">
                                            </div>
                                            </div>
                                            <div class="col">
                                            <div class="form-group">
                                              <label for="anios_calificados">Numero de Años Calificados</label>
                                              <input name="anios_calificados" type="text" class="form-control" id="anios_calificados" placeholder="Años Calificados" onkeypress="return valida(event)" value="<%=(newemp[0].anios_calificados!=null) ? newemp[0].anios_calificados:'0' %>">
                                            </div>
                                            </div>
                                            </div>

                  

                  <br><br><br><br><br><br><br><br><br>
            
            </div>
            </div>
        <!-- tab identificacion personal ends -->
        <!-- tab datospersonales1 -->

        <div class="tab-pane fade" id="datospersonales1" role="tabpanel" aria-labelledby="dropdown1-tab">
          <div class="container">
          <h1 class="card-title">Datos Laborales II</h1>


          <div class="row">
                <div class="col">
                <div class="form-group">
                <label for="nro_declar_jurada">N° de Declaracion Jurada</label>
                <input name="nro_declar_jurada" type="text" class="form-control" id="ndecjurada" placeholder="N° Dde Declaracion Jurada" onkeypress="return valida(event)" value="<%=(newemp[0].nro_declar_jurada!=null) ? newemp[0].nro_declar_jurada:'0' %>">
              </div>
              </div>
              <div class="col">
              <div class="form-group">
                <label for="nro_cuenta">N° de Cuenta Bancaria</label>
                <input name="nro_cuenta" type="text" class="form-control" id="ncbancaria" placeholder="N° de Cuenta Bancaria" onkeypress="return valida(event)" value="<%=(newemp[0].nro_cuenta!=null) ? newemp[0].nro_cuenta:'0' %>">
              </div>
              </div>
              </div>


              <div class="row">
              <div class="col">
              <div class="form-group">
                <label for="name">Nombre del Banco</label>
                <input name="banco" type="text" class="form-control" id="nbanco" placeholder="Nombre del Banco" minlength="2" value="<%=(newemp[0].banco!=null) ? newemp[0].banco:'' %>">
              </div>
              </div>
              <div class="col">
              <div class="form-group">
                <label for="name">Tipo de Cuenta</label>
                <select name="tipo_cuenta" class="form-control" style="width:100%">
                  <option value="<%=(newemp[0].tipo_cuenta!=null) ? newemp[0].tipo_cuenta:'' %>"><%=(newemp[0].tipo_cuenta!=null) ? newemp[0].tipo_cuenta:'' %></option>
                        <option value="CCMN">Cuenta Corriente M/N</option>
                        <option value="CCME">Cuenta Corriente M/E</option>
                        <option value="CAMN">Caja de Ahorros M/N</option>
                        <option value="CAME">Caja de Ahorros M/E</option>
                        </select>
              </div>
              </div>
              </div>

              </div>
              </div>
            <!-- tab datospersonales1 ends -->
                        <!-- tab datospersonales2 -->

                        <div class="tab-pane fade" id="datospersonales2" role="tabpanel" aria-labelledby="dropdown1-tab">
                            <div class="container">
                        <h1 class="card-title">Estudios</h1>
                       
                                  <div class="row">
                                          <div class="col">
                                          <div class="form-group">
                                            <label for="nivel">Nivel de Formacion</label>
                                            <select name="nivel" class="form-control" style="width:100%">
                                              <option value="<%=(newest[0].nivel!=null) ? newest[0].nivel:'' %>"><%=(newest[0].nivel!=null) ? newest[0].nivel:'' %></option>
                                                  <option value="Bachiller">Bachillerato</option>
                                                  <option value="Licenciatura">Licenciatura</option>
                                                  <option value="Maestria">Maestria</option>
                                                  <option value="Doctorado">Doctorado</option>
                                                  <option value="Otro">Otro</option>
                                                  </select>
                                                  <label for="especificacion">Especificacion</label>
                                                  <input name="especificacion" type="text" class="form-control" id="especificar" placeholder="especificacion" minlength="2" value="<%=(newest[0].especificacion!=null) ? newest[0].especificacion:'' %>">
                                          </div>
                                          </div>
                                          <div class="col">
                                          <div class="form-group">
                                            <label for="titulo">Titulo Obtenido</label>
                                            <input name="titulo" type="text" class="form-control" id="tobtenido" placeholder="Titulo Obtenido" minlength="2" value="<%=(newest[0].titulo!=null) ? newest[0].titulo:'' %>">
                                          </div>
                                          </div>
                                          </div>
              
              
                                          <div class="row">
                                          <div class="col">
                                          <div class="form-group">
                                            <label for="carrera">Carrera</label>
                                            <input name="carrera" type="text" class="form-control" id="carrera" placeholder="Carrera" minlength="2" value="<%=(newest[0].carrera!=null) ? newest[0].carrera:'' %>">
                                          </div>
                                          </div>
                                          <div class="col"><div class="form-group">
                                            <label for="institucion">Institucion</label>
                                            <input name="institucion" type="text" class="form-control" id="institucion" placeholder="Institucion" minlength="2" value="<%=(newest[0].institucion!=null) ? newest[0].institucion:'' %>">
                                          </div>
                                          </div>
                                          </div>
              
                                          <div class="row">
                                          <div class="col-6">
                                          <div class="form-group">
                                          <label for="concluida">Concluida?</label>
                                            <div class="form-radio">
                                                  <label class="form-check-label">
                                                    <input name="conluida" type="radio" class="form-check-input" name="optionsRadios" id="concluida" value="true" <%=x%>>
                                                    SI
                                                  </label>
                                              </div>
                                              <div class="form-radio">
                                                  <label class="form-check-label">
                                                    <input name="concluida" type="radio" class="form-check-input" name="optionsRadios" id="concluida" value="false" <%=y%>>
                                                    NO
                                                  </label>
                                              </div>
                                          </div>
                                          </div>
                                          </div>
              
              
                                          <div class="row">
                                          <div class="col">
                                          <div class="form-group">
                                          <label for="ciudad">Ciudad</label>
                                          <input name="ciudad" type="text" class="form-control" id="ciudad" placeholder="Ciudad" minlength="2" value="<%=(newest[0].ciudad!=null) ? newest[0].ciudad:'' %>">
                                          </div>
                                          </div>
                                          <div class="col">
                                          <div class="form-group">
                                          <label for="pais">Pais</label>
                                          <input name="pais" type="text" class="form-control" id="Pais" placeholder="Pais" minlength="2" value="<%=(newest[0].pais!=null) ? newest[0].pais:'' %>">
                                          </div>
                                          </div>
                                          </div>
              
                                          <div class="row">
