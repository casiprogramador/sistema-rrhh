                                          <input name="anios_vencidos" type="text" class="form-control" id="ffin" placeholder="Fecha Fin" value="<%=(newest[0].anios_vencidos!=null) ? newest[0].anios_vencidos:'' %>">
                                          </div>
                                          </div>
                                          <div class="col-4">
                                          <div class="form-group">
                                          <label for="fecha_inicio">Fecha Inicio</label>
                                          <div class="input-group date datepicker b-l-blue">
                                              <input name="fecha_inicio" type="text" class="form-control" value="<%=moment(newest[0].fecha_inicio).format('YYYY/MM/DD')%>">
                                              <div class="input-group-addon">
                                                <span class="mdi mdi-calendar-blank"></span>
                                              </div>
                                            </div>
                                          </div>
                                          </div>
                                          <div class="col-4">
                                          <div class="form-group">
                                          <label for="fecha_fin">Fecha Fin</label>
                                          <div class="input-group date datepicker b-l-blue">
                                              <input name="fecha_fin" type="text" class="form-control" value="<%=moment(newest[0].fecha_fin).format('YYYY/MM/DD')%>">
                                              <div class="input-group-addon">
                                                <span class="mdi mdi-calendar-blank"></span>
                                              </div>
                                            </div>
                                          </div>
                                          </div>
                                          </div>
              
                                          <div class="row">
                                          <div class="col">
                                          <div class="form-group">
                                          <label for="col_profesional">Colegio Profesional</label>
                                          <input name="col_profesional" type="text" class="form-control" id="col_profesional" placeholder="Codigo Profesional" minlength="2" value="<%=(newest[0].colegio_prof!=null) ? newest[0].colegio_prof:'' %>">
                                          </div>
                                          </div>
                                          <div class="col">
                                          <div class="form-group">
                                          <label for="nro_registro_pro">N° Registro Profesional</label>
                                          <input name="nro_registro_pro" id="nro_registro_pro" rows="6" class="form-control" placeholder="N° Registro Profesional" minlength="2" onkeypress="return valida(event)" value="<%=(newest[0].nro_registro_prof!=null) ? newest[0].nro_registro_prof:'' %>">
                                          </div>
                                          </div>
                                          </div>
          
                                  

                                              <div class="row">
                                                  <div class="col">
                                                  <div class="form-group">
                                                  <label for="address">Conocimientos</label>
                                                  <table border="0" class="table">
                                                      <tr>
                                                          <th>IDIOMA</th>
                                                          <th>LEE 1-100%</th>
                                                          <th>ESCRIBE</th>
                                                          <th>HABLA</th>
                                                      </tr>
                                                      <tr>
                                                          <td>Español</td>
                                                          <td><input name="lecturaesp" id="leeesp" rows="6" class="form-control" placeholder="%" onkeypress="return valida(event)" value="<%=newidioma[1].lee%>"></td>
                                                          <td><input name="escrituraesp" id="escribeesp" rows="6" class="form-control" placeholder="%" onkeypress="return valida(event)" value="<%=newidioma[1].escribe%>"></td>
                                                          <td><input name="hablaesp" id="hablaesp" rows="6" class="form-control" placeholder="%" onkeypress="return valida(event)" value="<%=newidioma[1].habla%>"></td>
                                                      </tr>
                                                      <tr>
                                                              <td>Ingles</td>
                                                              <td><input name="lecturaing" id="leeing" rows="6" class="form-control" placeholder="%" onkeypress="return valida(event)" value="<%=newidioma[2].lee%>"></td>
                                                              <td><input name="escrituraing" id="escribeing" rows="6" class="form-control" placeholder="%" onkeypress="return valida(event)" value="<%=newidioma[2].escribe%>"></td>
                                                              <td><input name="hablaing" id="hablaing" rows="6" class="form-control" placeholder="%" onkeypress="return valida(event)" value="<%=newidioma[2].habla%>"></td>
                                                      </tr>
                                                      <tr>
                                                        <td><select name="otro" class="form-control" id="idioma" name="idioma">

                                                                <% for(var i=0; i< newidioma.length; i++) { %>
                                                                     <option value="<%= newidioma[i].id %>"><%= newidioma[i].idioma %></option>
                                                                 <% } %>
                                                            </select></td>
                                                              <td><input name="lecturaotro" id="leeaotro" rows="6" class="form-control" placeholder="%" onkeypress="return valida(event)"value="<%=newidioma[0].lee%>"></td>
                                                              <td><input name="escrituraotro" id="escribeotro" rows="6" class="form-control" placeholder="%" onkeypress="return valida(event)" value="<%=newidioma[0].escribe%>"></td>
                                                              <td><input name="hablaotro" id="hablaotro" rows="6" class="form-control" placeholder="%" onkeypress="return valida(event)" value="<%=newidioma[0].habla%>"></td>
                                                      </tr>
                                                      </table>
                                                  </div>
                                                  </div>
                                                  </div>
                                              
                                            

                                <div class="row">
                                        <div class="col">
                                        <div class="form-group">
                                        <button type="submit" class="btn btn-success mr-2">Guardar</button>
                                        <button class="btn btn-outline-danger">Cancelar</button>
                                        </div>
                                        </div>
                                        </div> 
                          </div>
                        </div>
              </div>
              </fieldset>
                  </form>

                </div>
          </div>
      </div>
      </div>
      </div>
    </div>
    </div>
